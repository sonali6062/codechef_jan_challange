#include<iostream>
using namespace std;
int main()
{
	int T;
	cin>>T;
	for(int i=0;i<T;i++)
	{
		int N;
		long long int K,D;
		cin>>N>>K>>D;
		int a[N],sum=0,count=0;
		
		for(int j=0;j<N;j++)
		{
			cin>>a[j];
			
		}
		for(int j=0;j<N;j++)
		{
			sum=sum+a[j];
		}
		if(sum<K)
		{
			count=0;
		}
		else
		{
			count=sum/K;
		}
		if(count<=D )
		{
			cout<<count<<endl;
		}
		else
		cout<<D<<endl;
	    
		
	}
	return 0;
}
